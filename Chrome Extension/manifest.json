{
  "manifest_version": 2,
"name": "Misinformation Tracker",
"description": "Testing Plugin",
"version": "1.0",
"permissions": [
	"<all_urls>"
  ],
"content_scripts":[
    {
      "matches":[
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
"browser_action": {
  },
"background": {
      "scripts": ["background.js"],
      "persistent": false
    }
}